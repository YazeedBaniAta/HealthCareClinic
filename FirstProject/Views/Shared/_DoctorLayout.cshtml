<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
  
  <!-- Favicon -->
  <link rel="shortcut icon" type="image/x-icon" href="~/assets/images/logo.png" />
  <!-- Bootstrap -->
    <link href="~/assetsdashboard/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="~/assetsdashboard/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <!-- NProgress -->
    <link href="~/assetsdashboard/vendors/nprogress/nprogress.css" rel="stylesheet">
    <!-- bootstrap-daterangepicker -->
    <link href="~/assetsdashboard/vendors/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet">
    <!-- jQuery custom content scroller -->
    <link href="~/assetsdashboard/vendors/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.min.css" rel="stylesheet"/>
    <!-- iCheck -->
    <link href="~/assetsdashboard/vendors/iCheck/skins/flat/green.css" rel="stylesheet">
    <!-- Datatables -->
    <link href="~/assetsdashboard/vendors/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet">
    <link href="~/assetsdashboard/vendors/datatables.net-buttons-bs/css/buttons.bootstrap.min.css" rel="stylesheet">
    <link href="~/assetsdashboard/vendors/datatables.net-fixedheader-bs/css/fixedHeader.bootstrap.min.css" rel="stylesheet">
    <link href="~/assetsdashboard/vendors/datatables.net-responsive-bs/css/responsive.bootstrap.min.css" rel="stylesheet">
    <link href="~/assetsdashboard/vendors/datatables.net-scroller-bs/css/scroller.bootstrap.min.css" rel="stylesheet">
     <!-- FullCalendar -->
    <link href="~/assetsdashboard/vendors/fullcalendar/dist/fullcalendar.min.css" rel="stylesheet">
    <link href="~/assetsdashboard/vendors/fullcalendar/dist/fullcalendar.print.css" rel="stylesheet" media="print">

    <link href="~/assetsdashboard/vendors/select2/dist/css/select2.min.css" rel="stylesheet">
    <link href="~/assetsdashboard/vendors/select2/dist/css/select2.css" rel="stylesheet">

    <!-- Custom Theme Style -->
    <link href="~/assetsdashboard/css/custom.css" rel="stylesheet">
</head>
<body class="nav-md">
    <div class="container body">
      <div class="main_container">
        <div class="col-md-3 left_col menu_fixed">
          <div class="left_col scroll-view">
            <div class="navbar nav_title" style="border: 0;">
              <a asp-controller="DoctorDashboard" asp-action="Index" class="site_title"><img src="~/assets/images/logo.png" class="img-fluid"/> <span>Doctor Page</span></a>
            </div>

            <div class="clearfix"></div>

            <!-- menu profile quick info -->
            <div class="profile clearfix">
              <div class="profile_pic">
                <img src="~/Attachments/DoctorAttachments/@ViewBag.DoctorIamge" alt="..." class="img-circle profile_img">
              </div>
              <div class="profile_info">
                <span>Welcome,</span>
                <h2>Dr-@ViewBag.DoctorName</h2>
              </div>
            </div>
            <!-- /menu profile quick info -->

            <br />

            <!-- sidebar menu -->
            <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
              <div class="menu_section">
                  <h3>Dashboard</h3>
                <ul class="nav side-menu">
                  <li>
                      <a asp-controller="DoctorDashboard" asp-action="Index"><i class="fa fa-home"></i> Home</a>
                  </li>
                  </ul>
              </div>
                <div class="menu_section">
                <h3>General</h3>
                <ul class="nav side-menu">           
                  <li><a><i class="fa fa-check-square-o"></i> attendance <span class="fa fa-chevron-down"></span></a>
                    <ul class="nav child_menu">                 
                      <li><a asp-controller="DoctorDashboard" asp-action="AddAttendances" asp-route-id="@ViewBag.DoctorId">Make attendance</a></li>
                    </ul>
                  </li>
                                    
                  <li><a><i class="fa fa-edit"></i> appointment <span class="fa fa-chevron-down"></span></a>
                    <ul class="nav child_menu">
                      <li><a asp-controller="DoctorDashboard" asp-action="Appointments" asp-route-id="@ViewBag.DoctorId">View All appointments</a></li>
                       <li><a asp-controller="DoctorDashboard" asp-action="SearchAppointments">Search appointment</a></li>
                    </ul>
                  </li>

                </ul>
              </div>
              
              <div class="menu_section">
                <h3>Seetings</h3>
                <ul class="nav side-menu">
                   <li>
                      <a asp-controller="DoctorDashboard" asp-action="DoctorProfile" asp-route-id="@ViewBag.DoctorId"><i class="fa fa-user-md"></i> Profile</a>
                  </li>
                  </ul>
              </div>

            </div>
            <!-- /sidebar menu -->
          </div>
        </div>

        <!-- top navigation -->
        <div class="top_nav">
            <div class="nav_menu">
                <div class="nav toggle">
                  <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                </div>
                <nav class="nav navbar-nav">
                <ul class=" navbar-right">
                  <li class="nav-item dropdown open" style="padding-left: 15px;">
                    <a href="javascript:;" class="user-profile dropdown-toggle text-white" aria-haspopup="true" id="navbarDropdown" data-toggle="dropdown" aria-expanded="false">
                      <img src="~/Attachments/DoctorAttachments/@ViewBag.DoctorIamge" alt=""><span class="text-white">Dr-@ViewBag.DoctorName</span>
                    </a>
                    <div class="dropdown-menu dropdown-usermenu pull-right" aria-labelledby="navbarDropdown">
                      <a class="dropdown-item"  asp-controller="DoctorDashboard" asp-action="DoctorProfile" asp-route-id="@ViewBag.DoctorId"><i class="fa fa-user-md pull-right"></i> Profile</a>
                      <a class="dropdown-item"  asp-controller="DoctorDashboard" asp-action="DoctorProfile" asp-route-id="@ViewBag.DoctorId"><i class="fa fa-gear pull-right"></i> Settings </a>
                      <a class="dropdown-item"  asp-controller="DoctorDashboard" asp-action="LogOut"><i class="fa fa-sign-out pull-right"></i> Log Out</a>
                    </div>
                  </li>
                </ul>
              </nav>
            </div>
          </div>
        <!-- /top navigation -->

        <!-- page content -->
        <div class="right_col" role="main">
          <div class="">
                @RenderBody()
          </div>
        </div>
        <!-- /page content -->

        <!-- footer content -->
        <footer>
           <div class="copyright text-center">
            &copy; Copyright
            <strong>
                <span id="copyright">
                    <script>document.getElementById('copyright').appendChild(document.createTextNode(new Date().getFullYear()))</script>
                </span>
            </strong>. All Rights Reserved
        </div>
          <div class="clearfix"></div>
        </footer>
        <!-- /footer content -->
      </div>
    </div>






      <!-- jQuery -->
    <script src="~/assetsdashboard/vendors/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap -->
   <script src="~/assetsdashboard/vendors/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
   <!-- FastClick -->
    <script src="~/assetsdashboard/vendors/fastclick/lib/fastclick.js"></script>
    <!-- NProgress -->
    <script src="~/assetsdashboard/vendors/nprogress/nprogress.js"></script>
    <!-- jQuery custom content scroller -->
    <script src="~/assetsdashboard/vendors/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js"></script>
    <!-- jQuery Sparklines -->
    <script src="~/assetsdashboard/vendors/jquery-sparkline/dist/jquery.sparkline.min.js"></script>
    <!-- DateJS -->
    <script src="~/assetsdashboard/vendors/DateJS/build/date.js"></script>
    <!-- bootstrap-daterangepicker -->
    <script src="~/assetsdashboard/vendors/moment/min/moment.min.js"></script>
    <script src="~/assetsdashboard/vendors/bootstrap-daterangepicker/daterangepicker.js"></script>
    <!-- iCheck -->
    <script src="~/assetsdashboard/vendors/iCheck/icheck.min.js"></script>
    <!-- Datatables -->
    <script src="~/assetsdashboard/vendors/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/assetsdashboard/vendors/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
    <script src="~/assetsdashboard/vendors/datatables.net-buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/assetsdashboard/vendors/datatables.net-buttons-bs/js/buttons.bootstrap.min.js"></script>
    <script src="~/assetsdashboard/vendors/datatables.net-buttons/js/buttons.flash.min.js"></script>
    <script src="~/assetsdashboard/vendors/datatables.net-buttons/js/buttons.html5.min.js"></script>
    <script src="~/assetsdashboard/vendors/datatables.net-buttons/js/buttons.print.min.js"></script>
    <script src="~/assetsdashboard/vendors/datatables.net-fixedheader/js/dataTables.fixedHeader.min.js"></script>
    <script src="~/assetsdashboard/vendors/datatables.net-keytable/js/dataTables.keyTable.min.js"></script>
    <script src="~/assetsdashboard/vendors/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/assetsdashboard/vendors/datatables.net-responsive-bs/js/responsive.bootstrap.js"></script>
    <script src="~/assetsdashboard/vendors/datatables.net-scroller/js/dataTables.scroller.min.js"></script>
    <script src="~/assetsdashboard/vendors/jszip/dist/jszip.min.js"></script>
    <script src="~/assetsdashboard/vendors/pdfmake/build/pdfmake.min.js"></script>
    <script src="~/assetsdashboard/vendors/pdfmake/build/vfs_fonts.js"></script>
     <!-- FullCalendar -->
    <script src="~/assetsdashboard/vendors/moment/min/moment.min.js"></script>
    <script src="~/assetsdashboard/vendors/fullcalendar/dist/fullcalendar.min.js"></script>
    <!-- select2 -->
    <script src="~/assetsdashboard/vendors/select2/dist/js/select2.min.js"></script>
    <script src="~/assetsdashboard/vendors/select2/dist/js/select2.js"></script>
    <!-- Custom Theme Scripts -->
    <script src="~/assetsdashboard/js/custom.js"></script>

    @await Component.InvokeAsync("Notyf") 
    @RenderSection("Scripts", required: false)
</body>
</html>
